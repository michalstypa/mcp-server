{
  "name": "Backtick MCP server",
  "version": "1.0.0",
  "description": "Might be useful one day",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "keywords": [
    "mcp",
    "server",
    "backtick"
  ],
  "author": "Backtick",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.0",
    "express-prom-bundle": "^7.0.0",
    "pino": "^8.16.0",
    "prom-client": "^15.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "nock": "^13.4.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
